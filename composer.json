{
    "name": "talboterie/flysystem-gcp-storage",
    "type": "library",
    "description": "Flysystem adapter for Google Cloud Storage",
    "keywords": [
        "laravel",
        "Talboterie",
        "Flysystem GCP Storage"
    ],
    "homepage": "https://talboterie.dev/packages/flysystem-gcp-storage",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Talbot",
            "email": "vincent@talbot.ninja",
            "homepage": "https://vincenttalbot.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "google/cloud-storage": "^1.17",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "squizlabs/php_codesniffer": "~3.5"
    },
    "autoload": {
        "psr-4": {
            "Talboterie\\FlysystemGCPStorage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Talboterie\\FlysystemGCPStorage\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
